<UserControl x:Class="Addressbook.Views.ContactListView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Addressbook.Views"
             xmlns:viewModels="clr-namespace:Addressbook.Shared.ViewModels;assembly=Addressbook.Shared"
             xmlns:models="clr-namespace:Addressbook.Shared.Models;assembly=Addressbook.Shared"
             d:DataContext="{d:DesignInstance Type=viewModels:ContactAddViewModel}"
             
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <StackPanel>
            <Label Content="List of Contacts" />
            <Button Content="Navigate To Add Contact" Command="{Binding NavigateToAddCommand}" />
        </StackPanel>
    </Grid>
</UserControl>
